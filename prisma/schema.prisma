generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model EntityUser {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  createdAt DateTime @default(now())
}

model Evento {
  id            String   @id @default(uuid())
  tipoAccion    String
  timestamp     String
  usuario       String       
  correo        String?      
  numeroTelefono String?
  codigo        String?
  fecha         DateTime?
  
  @@map("eventos")
}